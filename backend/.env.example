# MySQL connection string:
# mysql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL="mysql://user:password@localhost:3306/youtube_drive_app"

PORT=4000

# Google Drive service account path (relative to backend root)
GOOGLE_APPLICATION_CREDENTIALS=./credentials/service-account.json

# Drive folder ID shared with the service account
DRIVE_FOLDER_ID=your_drive_folder_id_here
